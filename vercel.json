{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/dashboard", "dest": "/" },
    { "src": "/planner", "dest": "/" },
    { "src": "/workout", "dest": "/" },
    { "src": "/stats", "dest": "/" },
    { "src": "/weekly_summary", "dest": "/" },
    { "src": "/profile", "dest": "/" },
    { "src": "/register", "dest": "/" },
    { "src": "/api/(.*)", "dest": "/index.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
